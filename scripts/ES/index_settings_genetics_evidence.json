{
  "mappings": {
    "dynamic_templates": [
      {
        "nesteds": {
          "match_mapping_type": "object",
          "match": "facet_*",
          "mapping": {
            "type": "nested"
          }
        }
      }
    ],
    "properties": {
      "oddsRatio": {
        "type": "double"
      },
      "oddsRatioConfidenceIntervalUpper": {
        "type": "double"
      },
      "oddsRatioConfidenceIntervalLower": {
        "type": "double"
      }
    }
  },
  "settings": {
    "index": {
      "number_of_replicas": 0,
      "number_of_shards": 5
    }
  }
}